#!/bin/bash

CARTRIDGE_TYPE="diy-0.1"
export OPENSHIFT_CARTRIDGE_DIR="${OPENSHIFT_HOMEDIR}/${CARTRIDGE_TYPE}/"
export OPENSHIFT_VIRTUALENV_DIR="${OPENSHIFT_HOMEDIR}app-root/runtime/virtenv/"

export CAH_SERVER_PROXY_PORT="${OPENSHIFT_INTERNAL_PORT}"
export CAH_SERVER_DOMAIN="${OPENSHIFT_APP_DNS}"
export CAH_SERVER_PORT="80"
export CAH_SERVER_INTERFACE="${OPENSHIFT_INTERNAL_IP}"

export CAH_WEBSOCKET_DOMAIN="${OPENSHIFT_APP_DNS}"
export CAH_WEBSOCKET_PORT="8000"
